# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
end_of_line = lf
charset = utf-8
tab_width = 4
indent_style = tab
indent_size = tab
trim_trailing_whitespace = true
insert_final_newline = true

[{package.json,.travis.yml}]
indent_style = space
indent_size = 2
